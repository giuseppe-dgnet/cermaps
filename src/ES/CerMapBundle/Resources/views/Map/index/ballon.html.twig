<script type="text/html" id="template_empty_balloon">
    <div id="bl_{id}">
        Attendere prego...
    </div>
</script>
<script type="text/html" id="template_balloon">
    <div id="bl_{id}" style="width: 500px;" class="info_bubble">
        <div class="box-lightgray no-border" style="border-bottom: solid 1px #ccc !important;">
            <table width="100%" cellspacing="5">
                <tr>
                    <td width="25px" height="25px"><img src="/bundles/escermap/images/marker/{attivita_principale}.png" style="width:18px; height: 24px;" class="left"></td>
                    <td><h3>{h_attivita_principale} <small>{comune_testuale}</small></h3></td>
                </tr>
            </table>
        </div>
        <div class="text-small">
            <table width="100%" cellspacing="5" class="info_bubble_data">
                <tr>
                    <td width="340px">
                        <table width="100%">
                            <tr class="as">
                                <td width="135px" style="vertical-align: middle">Attivita secondarie</td>
                                <td height="25px">{elenco_attivita}</td>
                            </tr>
                            <tr>
                                <td width="135px">Ragione sociale</td>
                                <td><strong>{ragione_sociale}</strong></td>
                            </tr>
                            <tr>
                                <td width="135px">Descrizione</td>
                                <td>({descrizione_attivita})</td>
                            </tr>
                            <tr>
                                <td width="135px">Codici CER</td>
                                <td><strong class="test">{cer}</strong></td>
                            </tr>
                            <tr>
                                <td width="135px">Codici CER in conferimento</td>
                                <td><strong class="test">{cer_trattati}</strong></td>
                            </tr>
                            <tr>
                                <td width="135px">Recuperabili</td>
                                <td><strong class="test">{tipologie}</strong></td>
                            </tr>
                            <tr>
                                <td width="135px">Materie prime seconde</td>
                                <td><strong class="test">{mps}</strong></td>
                            </tr>
                            <tr>
                                <td width="135px">Servizi</td>
                                <td><strong class="test">{servizi_sr}</strong></td>
                            </tr>
                            <tr>
                                <td width="135px">Indirizzo</td>
                                <td><strong>{indirizzo}</strong></td>
                            </tr>
                            <tr>
                                <td width="135px">Distanza</td>
                                <td><strong>{distanza}</strong></td>
                            </tr>
                        </table>
                    </td>
                    <td valign="top" style="width: 150px;">
                        <img class="rounded-3 left" style="width: 150px; height: 150px;" src="{logo}">
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        {rdo}
                        <div class="right relative">
                            {telefono}
                            {slug}
                        </div>
                    </td>
                </tr>
            </table>
        </div>
    </div>
</script>
